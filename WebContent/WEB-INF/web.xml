<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>disaster-management</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<description></description>
		<display-name>CreateIncidentServlet</display-name>
		<servlet-name>CreateIncidentServlet</servlet-name>
		<servlet-class>com.sundar.disastermanagement.servlet.CreateIncidentServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>ViewIncidentServlet</display-name>
		<servlet-name>ViewIncidentServlet</servlet-name>
		<servlet-class>com.sundar.disastermanagement.servlet.ViewIncidentServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>GetVillageList</display-name>
		<servlet-name>GetVillageList</servlet-name>
		<servlet-class>com.sundar.disastermanagement.servlet.GetVillageList</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreateIncidentServlet</servlet-name>
		<url-pattern>/createIncident</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ViewIncidentServlet</servlet-name>
		<url-pattern>/viewIncident</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GetVillageList</servlet-name>
		<url-pattern>/getVillageList</url-pattern>
	</servlet-mapping>
</web-app>
